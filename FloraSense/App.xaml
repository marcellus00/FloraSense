<Application
    x:Class="FloraSense.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:floraSense="using:FloraSense">

    <Application.Resources>
        <floraSense:BooleanToVisibilityConverter x:Key="BooleanToVisibility"/>
        <floraSense:MacConverter x:Key="MacConverter"/>
        <floraSense:PercentFormatter x:Key="PercentFormatter"/>
        <floraSense:StringToVisibilityConverter x:Key="StringToVisibility"/>
    </Application.Resources>
</Application>
